option_settings:
  aws:elasticbeanstalk:environment:EnvironmentType: MultiContainer
  aws:elasticbeanstalk:container:python:
    WSGIPath: application.py
    NumProcesses: 1
    NumThreads: 15
  aws:elasticbeanstalk:container:python:staticfiles:
    /static/: static/

container_commands:
  01_wsgi_reload:
    command: "echo WSGI application reloaded"
